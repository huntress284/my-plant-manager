    hr

    #exampleModal.modal.fade(tabindex='-1', aria-labelledby='exampleModalLabel', aria-hidden='true')
        .modal-dialog
            .modal-content
                .modal-header
                    h1#exampleModalLabel.modal-title.fs-5 Add New Plant
                    button.btn-close(type='button', data-bs-dismiss='modal', aria-label='Close')
                .modal-body
                    form#form1
                        .mb-3
                            label.form-label(for='plantId') Plant ID
                            input#plantId.form-control(type='text', aria-describedby='plantName', placeholder='Plant ID')
                        .mb-3
                            label.form-label(for='plantInput') Plant Name
                            input#plantInput.form-control(type='text', placeholder='Plant Name')
                    form#form2(method='POST', action='/nursery', enctype='multipart/form-data')
                        .mb-3
                            label.form-label(for='photo') Again..
                            input#photo(type='text', name='picname', required='')
                        .mb-3
                            label Upload profile picture
                            input(type='file', name='profile-file', required='')
                .modal-footer
                    button.btn.btn-secondary(type='button', data-bs-dismiss='modal') Close
                    button.btn#theButton(type='button', onclick="addBaby()") Save changes

    //p=data.length
    .row.row-cols-1.row-cols-md-3.g-4
        each plant of plants
            if plant[4] === 'baby'
                .col
                    .card
                        img.card-img-top(src='/static/images/' + plant[2] + '.jpg', alt=plant[2])
                        .card-body
                            h5.card-title= plant[2]
                            .card-text
                                //.d-flex.flex-row
                                lead= plant[5]
                                    i.fa-solid.fa-cake-candles.mx-1
                                //lead Last Water
                                .lead Last Water&nbsp
                                    i.fa-solid.fa-droplet
                                p= plant[3]
                            hr
                            .d-flex.flex-column
                                //button.btn.btn-outline-secondary.my-1#toDelete(data-uuid= `${baby[0]}` onclick= `removeBaby()`) Delete Plant
                                button.btn.btn-outline-secondary.my-1#toWater(data-water= `${plant[0]}` onclick= `water(this.getAttribute('data-water'))`) Water! &nbsp;
                                    i.fa-solid.fa-droplet
                                .dropdown
                                    a.btn.btn-outline-secondary.dropdown-toggle(href='#', role='button', data-bs-toggle='dropdown', aria-expanded='false')
                                        | More
                                    ul.dropdown-menu
                                        li
                                            a.dropdown-item#greenhouse(data-greenhouse= `${plant[0]}` href='#' onclick= `greenhouse()` ) Move to Greenhouse &nbsp
                                                i.fa-solid.fa-house
                                        li
                                            a.dropdown-item#graveyard(data-graveyard= `${plant[0]}` href='#' onclick= `graveyard()`) Move to Graveyard &nbsp
                                                i.fa-solid.fa-plant-wilt
                                        li
                                            a.dropdown-item#toDelete(href='#' data-uuid= `${plant[0]}` onclick= `removePlant()`) Delete Plant &nbsp
                                                .fa-solid.fa-circle-xmark
